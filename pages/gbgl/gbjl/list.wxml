<!--pages/gbgl/gbjl/list.wxml-->
<view class="tool_bar_v" wx:if="{{showToolBarView}}">
  <view class="title_v">筛选页面</view>
  <view class="back_but_v" data-flag="{{false}}" catchtap="showToolBarView">{{backButSign}}</view>
  <view class="ddh_v">
    <span class="ddh_span">订单号</span>
    <input class="ddh_inp" id="ddh_inp" type="text" placeholder="请输入订单号" value="{{mc}}" bindinput="getInputValue"/>
  </view>
  <view class="cyclCph_v">
    <span class="cyclCph_span">车牌号</span>
    <input class="cyclCph_inp" id="cyclCph_inp" type="text" placeholder="请输入车牌号" value="{{cyclCph}}" bindinput="getInputValue"/>
  </view>
  <view class="gbksrq_v">
    <span class="gbksrq_span">过磅开始日期</span>
    <view class="gbksrq_picker_v">
      <picker class="gbksrq_picker" mode="date" bindcancel="pickerGbksrqCancel" bindchange="pickerGbksrqChange">{{gbksrq==''?gbksrqPlaceholder:gbksrq}}</picker>
    </view>
  </view>
  <view class="gbkssj_v">
    <span class="gbkssj_span">过磅开始时间</span>
    <view class="gbkssj_picker_v">
      <picker class="gbkssj_picker" mode="time" bindcancel="pickerGbkssjCancel" bindchange="pickerGbkssjChange">{{gbkssj==''?gbkssjPlaceholder:gbkssj}}</picker>
    </view>
  </view>
  <view class="gbjsrq_v">
    <span class="gbjsrq_span">过磅结束日期</span>
    <view class="gbjsrq_picker_v">
      <picker class="gbjsrq_picker" mode="date" bindcancel="pickerGbjsrqCancel" bindchange="pickerGbjsrqChange">{{gbjsrq==''?gbjsrqPlaceholder:gbjsrq}}</picker>
    </view>
  </view>
  <view class="gbjssj_v">
    <span class="gbjssj_span">过磅结束时间</span>
    <view class="gbjssj_picker_v">
      <picker class="gbjssj_picker" mode="time" bindcancel="pickerGbjssjCancel" bindchange="pickerGbjssjChange">{{gbjssj==''?gbjssjPlaceholder:gbjssj}}</picker>
    </view>
  </view>
  <view class="bottom_v">
    <view class="but_list_v">
      <view class="but_v confirm_but_v" catchtap="getListData">确定</view>
      <view class="but_v reset_but_v">重置</view>
    </view>
  </view>
</view>

<view class="main_v">
  <view class="title_v">列表页面</view>
  <view class="back_but_v" catchtap="goHomePage">{{backButSign}}</view>
  <view class="no_data_v" wx:if="{{showNoDataView}}">{{noDataText}}</view>
  <view class="list_v" wx:if="{{!showNoDataView}}">
    <view class="item_v" wx:for-items="{{gbjlList}}">
      <view class="row_v ddh_v">
        <view class="key_v">订单号</view>
        <view class="val_v">{{item.ddh}}</view>
      </view>
      <view class="row_v cyclCph_v">
        <view class="key_v">车牌号</view>
        <view class="val_v">{{item.cyclCph}}</view>
      </view>
      <view class="row_v gbzl_v">
        <view class="key_v">过磅重量</view>
        <view class="val_v">{{item.gbzl}}</view>
      </view>
      <view class="row_v gbztMc_v">
        <view class="key_v">过磅状态</view>
        <view class="val_v">{{item.gbztMc}}</view>
      </view>
      <view class="row_v gblxMc_v">
        <view class="key_v">过磅类型</view>
        <view class="val_v">{{item.gblxMc}}</view>
      </view>
      <view class="row_v gbsj_v">
        <view class="key_v">过磅时间</view>
        <view class="val_v">{{item.gbsj}}</view>
      </view>
      <view class="but_list_v">
        <view class="but_v detail_but_v" data-id="{{item.id}}" catchtap="goDetailPage">详情</view>
      </view>
    </view>
  </view>
  <view class="bottom_v">
    <a wx:if="{{showDjckgdView}}" data-flag="{{false}}" catchtap="showPageView">点击查看更多</a>
    <view class="page_v" wx:if="{{showPageView}}">
      <view class="data_count_v">
        共<text class="val_text">{{dataCount}}</text>条
      </view>
      <view class="pre_page_v click_v" data-flag="{{prePageFlag}}" wx:if="{{prePageEnable}}" catchtap="loadListDataByPageFlag">上一页</view>
      <view class="pre_page_v unClick_v" wx:if="{{!prePageEnable}}">上一页</view>
      <view class="page_count_v">
        <text class="curr_page_text">{{currentPage}}</text>/
        <text class="page_count_text">{{pageCount}}</text>
      </view>
      <view class="next_page_v click_v" data-flag="{{nextPageFlag}}" wx:if="{{nextPageEnable}}" catchtap="loadListDataByPageFlag">下一页</view>
      <view class="next_page_v unClick_v" wx:if="{{!nextPageEnable}}">下一页</view>
    </view>
    <view class="but_list_v">
      <view class="but_v screen_but_v" data-flag="{{true}}" catchtap="showToolBarView">筛选</view>
    </view>
  </view>
</view>
