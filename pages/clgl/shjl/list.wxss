/* pages/clgl/shjl/list.wxss */
.tool_bar_v{
  width: 100%;
  height: 100%;
  background-color: #fff;
  position: fixed;
  z-index: 1;
  overflow-y: auto;
}
.tool_bar_v .clCph_v,
.tool_bar_v .shrYhm_v,
.tool_bar_v .shksrq_v,
.tool_bar_v .shkssj_v,
.tool_bar_v .shjsrq_v,
.tool_bar_v .shjssj_v{
  width:100%;
  height:50px;
  line-height:50px;
}
.tool_bar_v .clCph_v .clCph_span,
.tool_bar_v .shrYhm_v .shrYhm_span,
.tool_bar_v .shksrq_v .shksrq_span,
.tool_bar_v .shkssj_v .shkssj_span,
.tool_bar_v .shjsrq_v .shjsrq_span,
.tool_bar_v .shjssj_v .shjssj_span{
  margin-left:10px;
}
.tool_bar_v .clCph_v .clCph_inp,
.tool_bar_v .shrYhm_v .shrYhm_inp{
  width:180px;
  margin-top:-35px;
  margin-left:120px;
  border: #eee solid 1px;
}
.tool_bar_v .shksrq_v .shksrq_picker_v,
.tool_bar_v .shkssj_v .shkssj_picker_v,
.tool_bar_v .shjsrq_v .shjsrq_picker_v,
.tool_bar_v .shjssj_v .shjssj_picker_v{
  width:170px;
  height:40px;
}
.tool_bar_v .shksrq_v .shksrq_picker_v .shksrq_picker,
.tool_bar_v .shkssj_v .shkssj_picker_v .shkssj_picker,
.tool_bar_v .shjsrq_v .shjsrq_picker_v .shjsrq_picker,
.tool_bar_v .shjssj_v .shjssj_picker_v .shjssj_picker{
  width:100%;
  height:40px;
  margin-top: -48px;
  margin-left: 123px;
  /*
  background-color: #00f;
  */
}
.tool_bar_v .bottom_v{
  width: 100%;
  height: 50px;
  background-color: #fff;
  bottom: 0;
  position: fixed;
}

.main_v{
  width: 100%;
}
.main_v .title_v,.tool_bar_v .title_v{
  width: 100%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 15px;
  font-weight: bold;
  background-color: #F8F8F8;
}
.main_v .back_but_v,.tool_bar_v .back_but_v{
  height: 40px;
  line-height: 40px;
  margin-top: -40px;
  margin-left: 10px;
}
.main_v .no_data_v{
  width: 100%;
  height:40px;
  line-height:40px;
  text-align: center;
}
.main_v .list_v{
  width: 95%;
  margin: auto;
}
.main_v .list_v .item_v{
  width: 100%;
  margin-top: 10px;
  border: #C7C7C7 solid 1px;
  border-radius:5px;
}
.main_v .list_v .item_v .row_v{
  width: 95%;
  margin: auto;
}
.main_v .list_v .item_v .clCph_v,
.main_v .list_v .item_v .shrYhm_v,
.main_v .list_v .item_v .shsj_v,
.main_v .list_v .item_v .shjgMc_v{
  border-bottom: #C9C9C9 solid 1px;
}
.main_v .list_v .item_v .key_v{
  width:100px;
  height:40px;
  line-height:40px;
  margin-left:8px;
  color:#666;
  text-align:center;
}
.main_v .list_v .item_v .val_v{
  height:40px;
  line-height:40px;
  margin-top:-40px;
  margin-left:130px;
}
.main_v .list_v .item_v .but_list_v{
  width: 95%;
  height: 40px;
  margin: auto;
}
.main_v .list_v .item_v .but_list_v .but_v{
  width: 63px;
  height: 30px;
  line-height: 30px;
  margin-top: 10px;
  font-size: 11px;
  background-color: #F8F8F8;
  border: #C8C8C8 solid 1px;
  border-radius:3px;
}
.main_v .list_v .item_v .but_list_v .but_v .but_img{
  width:20px;
  height:20px;
  margin-top:5px;
  margin-left:5px;
}
.main_v .list_v .item_v .but_list_v .but_v .but_text{
  margin-left:5px;
  position: absolute;
}
.space1_v{
  width: 100%;
  height: 80px;
}

.main_v .bottom_v{
  width: 100%;
  height: 70px;
  background-color: #F8F8F8;
  bottom: 0;
  position: fixed;
}
.main_v .bottom_v .page_v{
  width: 100%;
  height: 20px;
}
.main_v .bottom_v .page_v .pre_page_v{
  width: 50px;
  height: 20px;
  margin-top: -20px;
  margin-left: 80px;
}
.main_v .bottom_v .page_v .page_count_v{
  margin-top: -20px;
  margin-left:150px;
}
.main_v .bottom_v .page_v .curr_page_text{
  color: #027999;
}
.main_v .bottom_v .page_v .next_page_v{
  width: 50px;
  height: 20px;
  float: right;
  margin-top: -20px;
  margin-right: 10px;
}
.main_v .bottom_v .page_v .click_v{
  color: #000;
}
.main_v .bottom_v .page_v .unClick_v{
  color: #666;
}

.main_v .bottom_v .but_list_v,
.tool_bar_v .bottom_v .but_list_v{
  width: 95%;
  margin: auto;
}
.main_v .bottom_v .but_list_v .but_v,
.tool_bar_v .bottom_v .but_list_v .but_v{
  width: 45%;
  height: 30px;
  line-height: 30px;
  margin-top: 10px;
  text-align: center;
  border: #BFBFBF solid 1px;
  border-radius:3px;
}
.tool_bar_v .bottom_v .but_list_v .reset_but_v{
  float: right;
  margin-top:-32px;
}